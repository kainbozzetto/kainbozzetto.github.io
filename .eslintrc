{
    "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "sourceType": "module"
  },

  "env": {
    "es6": true,
    "node": true
  },

  "plugins": [
    "standard",
    "promise"
  ],

  "globals": {
    "document": false,
    "navigator": false,
    "window": false
  },

  "extends": "eslint:recommended",
  "rules": {
    "no-console": "off"
  }
}